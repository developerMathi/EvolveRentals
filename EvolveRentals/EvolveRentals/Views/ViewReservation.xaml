                                                VerticalOptions="Center" />

                                                <Label
                                                Grid.Row="0"
                                                Grid.Column="1"
                                                FontSize="Small"
                                                HorizontalOptions="Start"
                                                Text="{Binding Email}"
                                                TextColor="{StaticResource BlackColor}"
                                                VerticalOptions="Center" />
                                                <Label
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                FontSize="Small"
                                                HorizontalOptions="Start"
                                                Text="{Binding DriverLicenseNumber}"
                                                TextColor="{StaticResource BlackColor}"
                                                VerticalOptions="Center" />
                                                <Label
                                                Grid.Row="2"
                                                Grid.Column="1"
                                                FontSize="Small"
                                                HorizontalOptions="Start"
                                                Text="{Binding hPhone}"
                                                TextColor="{StaticResource BlackColor}"
                                                VerticalOptions="Center" />

                                            </Grid>
                                        </renders:ExtendedViewCell>
                                    </DataTemplate>
                                </ListView.ItemTemplate>
                            </ListView>
                        </StackLayout>

                        <Grid
                        Grid.Row="1"
                        Margin="15,5,15,20"
