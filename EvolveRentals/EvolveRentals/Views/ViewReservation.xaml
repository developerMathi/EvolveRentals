<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="EvolveRentals.Views.ViewReservation"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:local="clr-namespace:EvolveRentals.MarkupExtensions"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:renders="clr-namespace:EvolveRentals.Renders"
    Title="Upcoming Reservation"
   NavigationPage.HasBackButton="False"
    mc:Ignorable="d"
    BackgroundColor="{AppThemeBinding Dark={StaticResource BlackColor}, Light={StaticResource BackGroundColor}}"
    xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"
            ios:Page.UseSafeArea="True">
    
    <ContentPage.Content>
        <Grid Margin="5,10,5,10"
              VerticalOptions="Fill">
            <Grid.RowDefinitions>
                <RowDefinition Height="50" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="0"
              HorizontalOptions="Fill"
                  BackgroundColor="Transparent">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="50" />
                </Grid.ColumnDefinitions>
                <Grid.HeightRequest>
                    <OnPlatform x:TypeArguments="x:Double">
                        <On Platform="iOS">
                            <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="50"
                                Tablet="60" />
                        </On>
                        <On Platform="Android,Windows">
                            <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="50"
                                Tablet="60" />
                        </On>
                    </OnPlatform>
                </Grid.HeightRequest>

                <ImageButton
                    x:Name="btnBack"
                    Margin="0"
                    Padding="0"
                    BackgroundColor="Transparent"
                    Clicked="btnBack_Clicked"
                    HeightRequest="30"
                    Source="{local:EmbeddedImage ResourceId=EvolveRentals.Assets.iconBack.png}"
                    VerticalOptions="CenterAndExpand" 
                Aspect="AspectFit"/>

                <Label
                    Grid.Column="1"
                    FontFamily="Roboto-Bold"
                    HorizontalOptions="CenterAndExpand"
                    TextColor="{StaticResource PrimaryColor}"
                    VerticalOptions="CenterAndExpand">
                    RESERVATION DETAILS
                    <Label.FontSize>
                        <OnPlatform x:TypeArguments="x:Double">
                            <On Platform="iOS">
                                <OnIdiom
                                    x:TypeArguments="x:Double"
                                    Phone="20"
                                    Tablet="24" />
                            </On>
                            <On Platform="Android,Windows">
                                <OnIdiom
                                    x:TypeArguments="x:Double"
                                    Phone="22"
                                    Tablet="26" />
                            </On>
                        </OnPlatform>
                    </Label.FontSize>
                </Label>
            </Grid>
            
            
            <ScrollView Grid.Row="1">
                <StackLayout>
                    <Grid Margin="0,20,0,0" RowSpacing="30">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>

                        <Grid Margin="0,0,0,30" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <Label
                            Grid.Row="0"
                            Grid.Column="0"
                            Margin="15,0,0,0"
                            FontFamily="Roboto-Bold"
                            Text="Reservation ID"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}"
                                VerticalOptions="Center">
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>

                            </Label>
                            <Label
                            x:Name="ReservationId"
                            Grid.Row="0"
                            Grid.Column="1"
                            Margin="0,0,15,0"
                            FontFamily="Roboto-Medium"
                            HorizontalTextAlignment="End"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}"
                                VerticalOptions="Center">
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>

                            </Label>

                            <Label VerticalOptions="Center"
                            Grid.Row="1"
                            Grid.Column="0"
                            Margin="15,0,0,0"
                            FontFamily="Roboto-Bold"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                Status
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>

                            </Label>
                            <Label VerticalOptions="Center"
                            x:Name="status"
                            Grid.Row="1"
                            Grid.Column="1"
                            Margin="0,0,15,0"
                            FontFamily="Roboto-Medium"
                            HorizontalTextAlignment="End"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>

                            </Label>

                            <Label VerticalOptions="Center"
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="15,0,0,0"
                            FontFamily="Roboto-Bold"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                Rental Days
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>

                            </Label>
                            <Label VerticalOptions="Center"
                            x:Name="NoOfDays"
                            Grid.Row="2"
                            Grid.Column="1"
                            Margin="0,0,15,0"
                            FontFamily="Roboto-Medium"
                            HorizontalTextAlignment="End"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>

                            </Label>

                            <Label VerticalOptions="Center"
                            Grid.Row="3"
                            Grid.Column="0"
                            Margin="15,0,0,0"
                            FontFamily="Roboto-Bold"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                Created Date Time
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>


                            </Label>
                            <Label VerticalOptions="Center"
                            x:Name="CreateDate"
                            Grid.Row="3"
                            Grid.Column="1"
                            Margin="0,0,15,0"
                            FontFamily="Roboto-Medium"
                            HorizontalTextAlignment="End"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>


                            </Label>

                            <Grid
                                Margin="0,15,0,0"
                            Grid.Row="4"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            BackgroundColor="{StaticResource PrimaryColor}"
                            HeightRequest="40">
                                <Grid.HeightRequest>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                            x:TypeArguments="x:Double"
                                            Phone="40"
                                            Tablet="50" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                            x:TypeArguments="x:Double"
                                            Phone="40"
                                            Tablet="50" />
                                        </On>
                                    </OnPlatform>
                                </Grid.HeightRequest>
                                <Label
                                FontFamily="Roboto-Bold"
                                HorizontalOptions="CenterAndExpand"
                                TextColor="{AppThemeBinding Light={StaticResource BackGroundColor}, Dark={StaticResource BlackColor}}"
                                VerticalOptions="CenterAndExpand">
                                    CAR INFORMATION
                                    <Label.FontSize>
                                        <OnPlatform x:TypeArguments="x:Double">
                                            <On Platform="iOS">
                                                <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="20"
                                                Tablet="24" />
                                            </On>
                                            <On Platform="Android,Windows">
                                                <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="22"
                                                Tablet="26" />
                                            </On>
                                        </OnPlatform>
                                    </Label.FontSize>
                                </Label>
                            </Grid>

                            <Grid
                            Grid.Row="5"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="15,0,15,0"
                            ColumnSpacing="10"
                            RowSpacing="0"
                            VerticalOptions="FillAndExpand">
                                <Grid.HeightRequest>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                            x:TypeArguments="x:Double"
                                            Phone="100"
                                            Tablet="120" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                            x:TypeArguments="x:Double"
                                            Phone="100"
                                            Tablet="120" />
                                        </On>
                                    </OnPlatform>
                                </Grid.HeightRequest>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition>
                                        <ColumnDefinition.Width>
                                            <OnPlatform x:TypeArguments="GridLength">
                                                <On Platform="iOS">
                                                    <OnIdiom
                                                    x:TypeArguments="GridLength"
                                                    Phone="100"
                                                    Tablet="150" />
                                                </On>
                                                <On Platform="Android,Windows">
                                                    <OnIdiom
                                                    x:TypeArguments="GridLength"
                                                    Phone="100"
                                                    Tablet="150" />
                                                </On>
                                            </OnPlatform>
                                        </ColumnDefinition.Width>
                                    </ColumnDefinition>
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Image
                                x:Name="carImage"
                                Grid.Row="0"
                                Grid.RowSpan="3" />

                                <Label
                                x:Name="vehicleSampleEntry"
                                Grid.Column="1"
                                Margin="0,0,0,5"
                                FontAttributes="Bold"
                                FontFamily="Roboto-Bold"
                                TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}"
                                    HorizontalTextAlignment="End" 
                                    VerticalOptions="Center">
                                    <Label.FontSize>
                                        <OnPlatform x:TypeArguments="x:Double">
                                            <On Platform="iOS">
                                                <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                            </On>
                                            <On Platform="Android,Windows">
                                                <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                            </On>
                                        </OnPlatform>
                                    </Label.FontSize>

                                </Label>

                                <StackLayout
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="0"
                                Padding="0"
                                Orientation="Horizontal"
                                    HorizontalOptions="EndAndExpand"
                                    VerticalOptions="Center"
                                    >
                                    <Label

                                    Margin="0,0,0,5"
                                    FontFamily="Roboto-Medium"
                                    Text="Vehicle Type: "
                                    TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}"
                                        VerticalOptions="Center"
                                        >
                                        <Label.FontSize>
                                            <OnPlatform x:TypeArguments="x:Double">
                                                <On Platform="iOS">
                                                    <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                                </On>
                                                <On Platform="Android,Windows">
                                                    <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                                </On>
                                            </OnPlatform>
                                        </Label.FontSize>

                                    </Label>
                                    <Label
                                    x:Name="VehicleType"
                                    Margin="0,0,0,5"
                                    FontFamily="Roboto-Medium"
                                    TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}"
                                        VerticalOptions="Center">
                                        <Label.FontSize>
                                            <OnPlatform x:TypeArguments="x:Double">
                                                <On Platform="iOS">
                                                    <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                                </On>
                                                <On Platform="Android,Windows">
                                                    <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                                </On>
                                            </OnPlatform>
                                        </Label.FontSize>

                                    </Label>
                                </StackLayout>
                                <StackLayout
                                Grid.Row="2"
                                Grid.Column="1"
                                Margin="0,0,0,0"
                                Padding="0"
                                Orientation="Horizontal"
                                Spacing="15"
                                    HorizontalOptions="EndAndExpand"
                                    VerticalOptions="Center">
                                    <StackLayout
                                    Grid.Column="0"
                                    Margin="0"
                                    Padding="0"
                                    Orientation="Horizontal"
                                    Spacing="5"
                                         >
                                        <Image
                                        BackgroundColor="Transparent"
                                        HorizontalOptions="Center"
                                        Source="{local:EmbeddedImage ResourceId=EvolveRentals.Assets.iconGraySeat.png}"
                                        VerticalOptions="CenterAndExpand">
                                            <Image.HeightRequest>
                                                <OnPlatform x:TypeArguments="x:Double">
                                                    <On Platform="iOS">
                                                        <OnIdiom
                                                        x:TypeArguments="x:Double"
                                                        Phone="20"
                                                        Tablet="25" />
                                                    </On>
                                                    <On Platform="Android,Windows">
                                                        <OnIdiom
                                                        x:TypeArguments="x:Double"
                                                        Phone="20"
                                                        Tablet="25" />
                                                    </On>
                                                </OnPlatform>
                                            </Image.HeightRequest>
                                        </Image>
                                        <Label
                                        x:Name="seatCount"
                                        FontFamily="Roboto-Medium"
                                        TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}"
                                        VerticalOptions="CenterAndExpand">
                                            <Label.FontSize>
                                                <OnPlatform x:TypeArguments="x:Double">
                                                    <On Platform="iOS">
                                                        <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                                    </On>
                                                    <On Platform="Android,Windows">
                                                        <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                                    </On>
                                                </OnPlatform>
                                            </Label.FontSize>

                                        </Label>
                                    </StackLayout>

                                    <StackLayout
                                    Margin="0"
                                    Padding="0"
                                    Orientation="Horizontal"
                                    Spacing="5">
                                        <Image
                                        BackgroundColor="Transparent"
                                        HorizontalOptions="Center"
                                        Source="{local:EmbeddedImage ResourceId=EvolveRentals.Assets.iconGrayBag.png}"
                                        VerticalOptions="CenterAndExpand">
                                            <Image.HeightRequest>
                                                <OnPlatform x:TypeArguments="x:Double">
                                                    <On Platform="iOS">
                                                        <OnIdiom
                                                        x:TypeArguments="x:Double"
                                                        Phone="20"
                                                        Tablet="25" />
                                                    </On>
                                                    <On Platform="Android,Windows">
                                                        <OnIdiom
                                                        x:TypeArguments="x:Double"
                                                        Phone="20"
                                                        Tablet="25" />
                                                    </On>
                                                </OnPlatform>
                                            </Image.HeightRequest>
                                        </Image>
                                        <Label
                                        x:Name="bagCount"
                                        FontFamily="Roboto-Medium"
                                        TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}"
                                        VerticalOptions="CenterAndExpand">
                                            <Label.FontSize>
                                                <OnPlatform x:TypeArguments="x:Double">
                                                    <On Platform="iOS">
                                                        <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                                    </On>
                                                    <On Platform="Android,Windows">
                                                        <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                                    </On>
                                                </OnPlatform>
                                            </Label.FontSize>

                                        </Label>
                                    </StackLayout>


                                    <StackLayout
                                    Margin="0"
                                    Padding="0"
                                    Orientation="Horizontal"
                                    Spacing="5">
                                        <Image
                                        BackgroundColor="Transparent"
                                        HorizontalOptions="Center"
                                        Source="{local:EmbeddedImage ResourceId=EvolveRentals.Assets.iconGrayFeature.png}"
                                        VerticalOptions="CenterAndExpand">
                                            <Image.HeightRequest>
                                                <OnPlatform x:TypeArguments="x:Double">
                                                    <On Platform="iOS">
                                                        <OnIdiom
                                                        x:TypeArguments="x:Double"
                                                        Phone="20"
                                                        Tablet="25" />
                                                    </On>
                                                    <On Platform="Android,Windows">
                                                        <OnIdiom
                                                        x:TypeArguments="x:Double"
                                                        Phone="20"
                                                        Tablet="25" />
                                                    </On>
                                                </OnPlatform>
                                            </Image.HeightRequest>
                                        </Image>
                                        <Label
                                        x:Name="transLabel"
                                        FontFamily="Roboto-Medium"
                                        TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}"
                                        VerticalOptions="CenterAndExpand">
                                            <Label.FontSize>
                                                <OnPlatform x:TypeArguments="x:Double">
                                                    <On Platform="iOS">
                                                        <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                                    </On>
                                                    <On Platform="Android,Windows">
                                                        <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                                    </On>
                                                </OnPlatform>
                                            </Label.FontSize>

                                        </Label>
                                    </StackLayout>


                                </StackLayout>

                            </Grid>

                            <Grid
                                 Margin="0,15,0,0"
                            Grid.Row="6"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            BackgroundColor="{StaticResource PrimaryColor}">
                                <Grid.HeightRequest>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                            x:TypeArguments="x:Double"
                                            Phone="40"
                                            Tablet="50" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                            x:TypeArguments="x:Double"
                                            Phone="40"
                                            Tablet="50" />
                                        </On>
                                    </OnPlatform>
                                </Grid.HeightRequest>
                                <Label
                                FontFamily="Roboto-Bold"
                                HorizontalOptions="CenterAndExpand"
                                TextColor="{AppThemeBinding Light={StaticResource BackGroundColor}, Dark={StaticResource BlackColor}}"
                                VerticalOptions="CenterAndExpand">
                                    PICKUP INFORMATION
                                    <Label.FontSize>
                                        <OnPlatform x:TypeArguments="x:Double">
                                            <On Platform="iOS">
                                                <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="20"
                                                Tablet="24" />
                                            </On>
                                            <On Platform="Android,Windows">
                                                <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="22"
                                                Tablet="26" />
                                            </On>
                                        </OnPlatform>
                                    </Label.FontSize>
                                </Label>
                            </Grid>

                            <Label VerticalOptions="Center"
                            Grid.Row="10"
                            Grid.Column="0"
                            Margin="15,0,0,0"
                            FontFamily="Roboto-Bold"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                Return Location
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>

                            </Label>
                            <Label VerticalOptions="Center"
                            x:Name="CheckInLocation"
                            Grid.Row="10"
                            Grid.Column="1"
                            Margin="0,0,15,0"
                            FontFamily="Roboto-Medium"
                            HorizontalTextAlignment="End"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
                                                Tablet="18" />
                                        </On>
                                        <On Platform="Android,Windows">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="15"
                                                Tablet="18" />
                                        </On>
                                    </OnPlatform>
                                </Label.FontSize>

                            </Label>

                            <Label VerticalOptions="Center"
                            Grid.Row="11"
                            Grid.Column="0"
                            Margin="15,0,0,0"
                            FontFamily="Roboto-Bold"
                            TextColor="{AppThemeBinding Dark={StaticResource BackGroundColor}, Light={StaticResource BlackColor}}">
                                Return Date/Time
                                <Label.FontSize>
                                    <OnPlatform x:TypeArguments="x:Double">
                                        <On Platform="iOS">
                                            <OnIdiom
                                                x:TypeArguments="x:Double"
                                                Phone="14"
